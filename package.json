{
  "name": "chainprompt-ai",
  "version": "1.1.2",
  "description": "Un applicazione server scritta in typescript con express e langchain per realizzare interessanti applicazioni prompt oriented.",
  "main": "dist/server.js",
  "type": "module",
  "publishConfig": {
    "registry": "http://alessandromodica.com:8081/repository/npm-cloud-pubblico/"
  },
  "scripts": {
    "publish:nexus": "npm publish",
    "clean": "rimraf dist",
    "build": "tsc",
    "start:prod": "node dist/server.js",
    "start": "node --no-warnings=ExperimentalWarning --loader ts-node/esm  src/backend/server.ts ./tsconfig.json",
    "start-dev": "nodemon --exec node --inspect=0.0.0.0:9229 --no-warnings=ExperimentalWarning --loader ts-node/esm  src/backend/server.ts ./tsconfig.json",
    "test": "jest",
    "testcoverage:backend": "./testcoverage.sh",
    "testcoverage:core": "./testcoverage.sh core",
    "testcoverage:full": "./testcoverage.sh full",
    "release": "npm version $npm_config_versione -m 'Rilasciata una nuova release backend' && git push",
    "patch": "npm version patch -m 'Rilasciata una nuova patch backend' && git push"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@huggingface/inference": "^4.13.3",
    "@huggingface/transformers": "^3.7.6",
    "@langchain/anthropic": "^1.0.0",
    "@langchain/community": "^1.0.3",
    "@langchain/core": "^1.0.2",
    "@langchain/google-genai": "^1.0.0",
    "@langchain/google-vertexai": "^1.0.1",
    "@langchain/langgraph": "^1.0.1",
    "@langchain/langgraph-checkpoint-postgres": "^1.0.0",
    "@langchain/ollama": "^1.0.0",
    "@langchain/openai": "^1.0.0",
    "@langchain/redis": "^1.0.0",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.6.8",
    "body-parser": "^1.20.2",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fs": "^0.0.1-security",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "kysely": "^0.28.8",
    "langchain": "^1.0.2",
    "mongoose": "^8.19.3",
    "nodemon": "^3.1.0",
    "path": "^0.12.7",
    "pg": "^8.16.3",
    "pretty": "^2.0.0",
    "redis": "^5.9.0",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "ts-node": "^10.9.2",
    "tsyringe": "^4.10.0",
    "undici": "^6.13.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ip-address": "^7.0.0",
    "@types/jest": "^29.5.14",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^20.12.7",
    "@types/pg": "^8.15.6",
    "@types/redis": "^4.0.10",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "rimraf": "^6.1.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  }
}